{
  "endpoints": [
    {
      "urlPath": "/real-time",
      "delaySeconds": 0
    },
    {
      "urlPath": "/delayed",
      "delaySeconds": 15
    }
  ],
  "maxBatchSize": 10,
  "port": 8090,
  "cache": {
    "maxAgeSeconds": 300
  },
  "logger": {
    "type": "pretty",
    "styling": "on",
    "minLevel": "debug"
  }
}
